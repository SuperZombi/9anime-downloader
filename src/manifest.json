{
   "manifest_version": 3,
   "version": "1.0.0",
   "name": "9Anime Downloader",
   "description": "Download anime from 9animetv",
   "permissions": ["webRequest", "tabs"],
   "host_permissions": [
      "https://9animetv.to/*"
   ],
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [{
      "matches": ["https://9animetv.to/watch/*"],
      "js": [
         "script.js", 
         "m3u8-parser.min.js"
      ],
      "run_at": "document_end"
   }]
}
